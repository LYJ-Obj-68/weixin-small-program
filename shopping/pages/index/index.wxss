.containers {
  background-color: #F2F2F2;
}
/*轮播图样式*/
.swiper_box {
  width: 100%;
}
swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
}
/*设置主标题样式*/
view.text {
  display: flex;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 12px;
  font-size: 15px;
  color: #656565;
}
.line_flag {
  width: 3px;
  height: 15px;
  display: inline-block;
  background-color: #f9f027;
}
view.text text {
  margin-left: 10px;
  line-height: 18px;
  font-weight: bold;
}
/*设置分类列表样式及布局*/
.venues_box {
